{
    "name": "dragon-code/laravel-route-names",
    "description": "Automatic generation of route names",
    "license": "MIT",
    "keywords": [
        "laravel",
        "dragon-code",
        "routes",
        "names",
        "naming",
        "routing"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@dragon-code.pro",
            "homepage": "https://github.com/andrey-helldar"
        }
    ],
    "support": {
        "issues": "https://github.com/TheDragonCode/laravel-route-names/issues",
        "source": "https://github.com/TheDragonCode/laravel-route-names"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "fig/http-message-util": "^1.1",
        "illuminate/routing": "^7.0 || ^8.0 || ^9.0"
    },
    "require-dev": {
        "dragon-code/extended-routes": "^3.1",
        "orchestra/testbench": "^5.0 || ^6.0 || ^7.0",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DragonCode\\LaravelRouteNames\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true,
            "dragon-code/codestyler": true,
            "friendsofphp/php-cs-fixer": true
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DragonCode\\LaravelRouteNames\\ServiceProvider"
            ]
        }
    }
}
